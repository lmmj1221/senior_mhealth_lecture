{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:vercel": "node scripts/build-production.js",
    "build:production": "node scripts/build-production.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:preview": "node scripts/deploy.js preview",
    "deploy:production": "node scripts/deploy.js production",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "analyze": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out dist"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "critters": "^0.0.23",
    "firebase": "^12.2.1",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.0.2",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "19.1.8",
    "@types/react-dom": "^19",
    "@vercel/analytics": "^1.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "firebase-admin": "^13.4.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "node-fetch": "^3.3.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "5.8.3",
    "web-vitals": "^4.2.4"
  }
}
